
public class recursion {
	public static void main (String[] args) {
		new fibonacci();
		new factorial();
		
		System.out.println(factorial.fact(5));
		
		int index = 0;
		//while this
		while (true)
		{
			System.out.println(fibonacci.fib(index));
			//index will increment the number so that 
			//the recursion is infinite.
			index++;
		}
	}
}
